{
	"manifest_version": 1,
	"name": "Migrants to people",
	"version": "0.1",
	"description": "An extension to re-humanise web content about migration.",
	"icons": { "16": "images/carbs16.png",
           "48": "images/carbs48.png",
          "128": "images/carbs128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"], 
			"js": ["myscript.js"],
			"run_at": "document_end"  
		}
	]
}
